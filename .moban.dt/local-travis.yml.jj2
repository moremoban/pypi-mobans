{% extends 'travis.yml.jj2' %}

{% block custom_python_versions %}
python:
  - 3.7
  - 2.7
{% endblock %}


{% block before_script %}
before_script:
  - find templates/ -type f -name '*.jj2' -exec echo '{}' \; |{% raw %}
    sed -e "s/templates\//{%include \"/" -e "s/$/\" %}/" > test.file
{% endraw %}
{% endblock %}

{% block script %}
  - moban -c config/data.yml -t test.file -td templates .
  - pip freeze
  - {{ test_command }}
{% endblock %}
